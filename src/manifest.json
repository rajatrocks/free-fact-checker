{
    "manifest_version": 3,
    "name": "Free Fact Checker - Use AI & Google Search to fact check any text for free",
    "description": "Select text on any web page or PDF and fact-check it instantly using Google Gemini with Search grounding",
    "version": "1.0.4",
    "minimum_chrome_version": "88",
    "permissions": [
        "storage",
        "contextMenus",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "Fact Check Selection",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "background": {
        "service_worker": "scripts/service-worker.js"
    },
    "options_page": "options.html",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["css/shadow.css", "images/*", "options.html"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "host_permissions": [
        "https://generativelanguage.googleapis.com/*"
    ],
    "key": ""    
}
